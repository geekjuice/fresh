{
  "name": "@geekjuice/fresh",
  "version": "1.0.0",
  "description": "fresh dependencies",
  "repository": "geekjuice/fresh",
  "author": {
    "name": "nicholas hwang",
    "email": "hello@undefined.engineer",
    "url": "https://undefined.engineer"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
  "bin": {
    "fresh": "index.js"
  },
  "keywords": [
    "geekjuice",
    "fresh",
    "modules",
    "packages",
    "dependencies",
    "evergreen",
    "latest",
    "edge",
    "cli"
  ],
  "dependencies": {
    "chalk": "2.3.0",
    "debug": "3.1.0",
    "inquirer": "4.0.1",
    "meow": "4.0.0",
    "ora": "1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "4.13.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-node": "5.2.1",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "6.0.0",
    "prettier": "1.9.1"
  },
  "scripts": {
    "eslint": "eslint",
    "jest": "jest",
    "precommit": "lint-staged",
    "prettier": "prettier --write"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run prettier",
      "npm run eslint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "prettier",
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "plugins": [
      "node"
    ],
    "rules": {
      "no-console": "off"
    }
  }
}
